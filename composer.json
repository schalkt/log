{
    "name": "schalkt/log",
    "version": "1.0.0",
    "description": "Simple and easy configurable PHP logfile system",
    "keywords": [
        "log",
        "php",
        "simple",
        "easy",
        "configurable"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Tamas Schalk",
        "homepage": "https://github.com/schalkt"
    }],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Schalkt\\Slog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Schalkt\\Slog\\Tests\\": "tests"
        }
    }
}